<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="UIKit" parent="TextAppearance.AppCompat">
        <item name="android:fontFamily">@font/uikit_regular</item>
        <item name="android:textColor">@color/dark_01</item>
    </style>

    <style name="Title_01" parent="UIKit">
        <item name="android:letterSpacing">0.02</item>
        <item name="android:textSize">44sp</item>
        <item name="android:fontFamily">@font/uikit_semi_bold</item>
        <item name="android:lineSpacingExtra">-12sp</item>
    </style>

    <style name="Title_05" parent="UIKit">
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textSize">24sp</item>
        <item name="android:lineSpacingExtra">-4sp</item>
        <item name="android:fontFamily">@font/uikit_semi_bold</item>
    </style>

    <style name="Title_09" parent="UIKit">
        <item name="android:letterSpacing">0</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">@font/uikit_semi_bold</item>
    </style>

    <style name="Title_10" parent="UIKit">
        <item name="android:letterSpacing">0</item>
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/uikit_bold</item>
    </style>

    <style name="Paragraph_03" parent="UIKit">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="Paragraph_04" parent="UIKit">
        <item name="android:textSize">10sp</item>
    </style>

</resources>