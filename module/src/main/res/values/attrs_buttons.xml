<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QuantityButton">
        <attr name="buttonType" format="enum">
            <enum name="editable" value="1" />
            <enum name="quantity" value="2" />
            <enum name="delete" value="3" />
        </attr>
        <attr name="defaultIcon" format="reference" />
        <attr name="value" format="integer" />
    </declare-styleable>
    <declare-styleable name="OrderTypeStatus">
        <attr name="orderType" format="enum">
            <enum name="restaurant" value="1" />
            <enum name="delivery" value="2" />
            <enum name="amount" value="3" />
        </attr>
        <attr name="title" format="string|reference" />
        <attr name="description" format="string|reference" />
    </declare-styleable>

</resources>